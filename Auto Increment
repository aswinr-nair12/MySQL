1) CREATE TABLE categories (category_id INT AUTO_INCREMENT PRIMARY KEY, category_name VARCHAR(100) NOT NULL);

2) CREATE TABLE books5 (book_id INT AUTO_INCREMENT PRIMARY KEY, book_name VARCHAR(150) NOT NULL, category_id INT, FOREIGN KEY (category_id) REFERENCES categories(category_id));

3) CREATE INDEX idx_book_name ON books5(book_name);

4) SHOW INDEX FROM books;
